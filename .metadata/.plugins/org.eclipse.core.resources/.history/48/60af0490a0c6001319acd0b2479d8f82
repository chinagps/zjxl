				maxlength : "应小于{0}个字符"
			},
			verifyCode:"验证码必填"
		},
		invalidHandler : function() {
			return false;
		},
		submitHandler : function() {
			sumberVehicles();
			return false;
		}
	});
	function sumberVehicles() {
		
		var atSite = $("#atSite").val();//所在地
		var fromProvince = $("#fromProvince option:selected").text();//线路出发省
		var fromCity = $("#fromCity option:selected").text();//出发市
		var toProvince = $("#toProvince option:selected").text();//所到省份
		var toCity = $("#toCity option:selected").text();//所到市
		var vehicleType = $("#vehicleType option:selected").text();//车辆类型
		var standardLoad = $("#standardLoad option:selected").text();//载重
		var vehicleTotal = $("#vehicleTotal").val();//自有车辆数量
		var contactName = $("#contactName").val();//联系人
		var contactType = $("#contactType").val();//联系方式
		var verifyCode = $("#verifyCode").val();
		var data = {
			"passageway.atSite" : atSite,
			"passageway.fromProvince" : fromProvince,
			"passageway.fromCity" : fromCity,
			"passageway.toProvince" : toProvince,
			"passageway.toCity" : toCity,
			"passageway.vehicleType" : vehicleType,
			"passageway.standardLoad" : standardLoad,
			"passageway.vehicleTotal" : vehicleTotal,
			"passageway.contactName" : contactName,
			"passageway.contactType" : contactType,
			"verifyCode":verifyCode
		}
		$.ajax({
			type : "POST",
			async : false,
			contenttype : "application/json; charset=utf-8",
			url : contextPath + 'front/passageway_passagewaySendMail.do',
			data : data,
			success : function(msg) {
				alert(msg);
				if(msg=="已提交"){
					resetForm("driversForm");//清空表单
				}
				$("#verifyCodeImg").trigger("click");//重新获取验证码
			},
			error : function() {
				
			}
		});
	}
	//初始化数据
	jQuery(document).ready(function() {
		getProvince();
		getStandardLoad();
		$("#uploadPicIframe").attr("src",LMIS_REQUEST_URL + "/pages/common/upload.jsp");
		
		//添加 车辆类型
		appendVehicleType("vehicleType");
	});

	// 获得额定载重类型
	function getStandardLoad() {
		var url = "/lmis-portal/sysCode_getDataTypeJSON.do?parentId=290351B127BF4703BAF2023D32E2F5E8";
		jQuery.getJSON(url, function(result) {
			setStandardLoad(result);
		});
	}

	// 设置额定载重
	function setStandardLoad(result) {
		var standardLoad = document.getElementById("standardLoad");
		jQuery.each(result.data, function(i, code) {
			var value = code.code;
			var text = code.name;
			var option = new Option(text, value);
			standardLoad.options.add(option);
		});
	}

	// 取所有省份
	function getProvince() {
		//&callback=?"注意这个是为了解决跨域访问的问题 
		var url = "/lmis-portal/sysAreaInfo_getAreaJSON.do?areaId=999999";
		jQuery.getJSON(url, function(result) {
			setFromProvince(result);
			setToProvince(result);
		});
	}

	// 出发地
	function setFromProvince(result) {
		var province = document.getElementById("fromProvince");
		jQuery.each(result.data, function(i, area) {
			var value = area.areaCode;
			var text = area.areaName;
			var option = new Option(text, value);
			province.options.add(option);
		});
	}

	// 按上级ID取子地区
	function getFromArea(name) {
		if (name == 'city') {
			clearSel(document.getElementById("fromCity")); //清空城市
			var province = document.getElementById("fromProvince");
			if (province.options[province.selectedIndex].value == "")
				return;
			var areaId = province.options[province.selectedIndex].value;
			var url = "/lmis-portal/sysAreaInfo_getAreaJSON.do?areaId="
					+ areaId;
			jQuery.getJSON(url, null, function call(result) {
				setFromCity(result);
			});
		} else if (name == 'county') {
			clearSel(document.getElementById("fromCounty")); //清空城区
			var city = document.getElementById("fromCity");
			if (city.options[city.selectedIndex].value == "")
				return;
			var areaId = city.options[city.selectedIndex].value;
			var url = "/lmis-portal/sysAreaInfo_getAreaJSON.do?areaId="
					+ areaId;
			jQuery.getJSON(url, null, function call(result) {
				setFromCounty(result);
			});
		}
	}

	function setFromCounty(result) {
		clearSel(document.getElementById("fromCounty")); //清空城区
		var county = document.getElementById("fromCounty");
		jQuery.each(result.data, function(i, area) {
			var value = area.areaCode;
			var text = area.areaName;
			var option = new Option(text, value);
			county.options.add(option);
		});
	}

	// 当改变省份时设置城市
	function setFromCity(result) {
		var city = document.getElementById("fromCity");
		jQuery.each(result.data, function(i, area) {
			var value = area.areaCode;
			var text = area.areaName;
			var option = new Option(text, value);
			city.options.add(option);
		});
	}

	// 目的地
	function setToProvince(result) {
		var province = document.getElementById("toProvince");
		jQuery.each(result.data, function(i, area) {
			var value = area.areaCode;
			var text = area.areaName;
			var option = new Option(text, value);
			province.options.add(option);
		});
	}

	// 按上级ID取子地区
	function getToArea(name) {
		if (name == 'city') {
			clearSel(document.getElementById("toCity")); //清空城市
			var province = document.getElementById("toProvince");
			if (province.options[province.selectedIndex].value == "")
				return;
			var areaId = province.options[province.selectedIndex].value;
			var url = "/lmis-portal/sysAreaInfo_getAreaJSON.do?areaId="
					+ areaId;
			jQuery.getJSON(url, null, function call(result) {
				setToCity(result);
			});
		} else if (name == 'county') {
			clearSel(document.getElementById("toCounty")); //清空城区
			var city = document.getElementById("toCity");
			if (city.options[city.selectedIndex].value == "")
				return;
			var areaId = city.options[city.selectedIndex].value;
			var url = "/lmis-portal/sysAreaInfo_getAreaJSON.do?areaId="
					+ areaId;
			jQuery.getJSON(url, null, function call(result) {
				setToCounty(result);
			});
		}
	}

	function setToCounty(result) {
		clearSel(document.getElementById("toCounty")); //清空城区
		var county = document.getElementById("toCounty");
		jQuery.each(result.data, function(i, area) {
			var text = area.areaName;
			var option = new Option(text, value);
			county.options.add(option);
		});
	}

	// 当改变省份时设置城市
	function setToCity(result) {
		var city = document.getElementById("toCity");
		jQuery.each(result.data, function(i, area) {
			var value = area.areaCode;
			var text = area.areaName;
			var option = new Option(text, value);
			city.options.add(option);
		});
	}

	// 清空下拉列表
	function clearSel(oSelect) {
		while (oSelect.childNodes.length > 0) {
			oSelect.removeChild(oSelect.childNodes[0]);
		}
	}
