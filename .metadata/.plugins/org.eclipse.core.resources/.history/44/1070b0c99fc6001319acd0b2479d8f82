<script type="text/javascript">
<!--

jQuery.validator.addMethod("mobile", function(value, element) {
    var length = value.length;
    var mobile =  /^(((13[0-9]{1})|(15[0-9]{1}))+\d{8})$/;
    return this.optional(element) || (length == 11 && mobile.test(value));
}, "手机号码格式错误");

$(document).ready(function() {
	$("#unionApplyForm").validate({
		rules: {
			fromProvince: "required",
			companyName:{required:true,maxlength:50},
			address:{required:true,maxlength:100},
			contactName:{required:true,maxlength:20},
			contactType:{required:true,maxlength:50},
			verifyCode:"required"
		},
		messages: {
			fromProvince: "请选择所在位置",
			companyName:{required:"请输入公司名称",maxlength:"应小于{0}个字符"},
			address:{required:"请输入您的地址",maxlength:"应小于{0}个字符"},
			contactName:{required:"请输入联系人姓名",maxlength:"应小于{0}个字符"},
			contactType:{required:"请输入联系方式",maxlength:"应小于{0}个字符"},
			verifyCode:"验证码必填"
		},
		invalidHandler : function(){
			return false;
		},
		submitHandler : function(){
			sumberUnionApply();
			return false;
		}
	});
});



function sumberUnionApply(){
	var province = $("#fromProvince option:selected").text();
	var city = $("#fromCity option:selected").text();
	var county = $("#fromCounty option:selected").text();
	var companyName = $("#companyName").val();
	var address = $("#address").val();
	var contactName = $("#contactName").val();
	var contactType = $("#contactType").val();
	var verifyCode = $("#verifyCode").val();
	var data  = {
			"unionApply.province":province,
			"unionApply.city":city,
			"unionApply.county":county,
			"unionApply.companyName":companyName,
			"unionApply.address":address,
			"unionApply.contactName":contactName,
			"unionApply.contactType":contactType,
			"verifyCode":verifyCode
	}
	$.ajax({
		type:"POST",
		async:false,
		contenttype: "application/json; charset=utf-8",
		url:contextPath+'front/unionApply_unionApplySendMail.do',
		data : data,
		success : function(msg){
			alert(msg);
			if(msg=="已提交"){
				resetForm("unionApplyForm");//清空表单
			}
			$("#verifyCodeImg").trigger("click");//重新获取验证码
		},
		error:function(data){
			
		}
	});
}

window.onload = function() {
     	  	  	initializeMenu("wylm_sub", "wylm");
     
     var clickNode = GetQueryString("clickNode");
		if (clickNode != null || clickNode != '') {
			$("#"+clickNode).click();
		}
}
