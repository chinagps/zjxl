    <script type="text/javascript">
        var first1 = true;//第一次加载客户列表标示
        
        //当前用户信息
        var SessionUserInfo = {
            TenantName: '体验账号',
            TenantLogoImgURl:' /Images/head_logo.png',
            IsExperienceLogin: 1,
            TenantType: 3,
            IsExistSelectedUser: 1,
            SelectedTenantName: '体验账号',
            IsSuper:'-1',
            IsShowDaoHang:''
        };

        $(document).ready(function () { //页面初始
            $('#currCus').text(SessionUserInfo.SelectedTenantName);

            $('.popuserInfo').mouseout(function () {
                $('.popuserInfo').hide()
            });
            $('.popuser,.popuserInfo').mouseover(function () {
                $('.popuserInfo').show()
            });
            //终端用户登录
            if (SessionUserInfo && SessionUserInfo.TenantType == 3) {
                $('#spanCurrentUser').hide();

                //终端用户才刷新
                reinitSmsCount();
                setTimeout(reinitSmsCount, 10000);
                //$('#spanChangeUser').hide();
            }
        });

        function reinitSmsCount() {
            $.ajax({
                url: appRootPath + "SiteMaster/Index",
                type: "post",
                data: {},
                success: function (data) {
                    var spanImg = $("#spanImg");
                    spanImg.empty();
                    var imgHtml = "";
                    if (data && data != null) {
                        if(data.SmsNum < 1)
                            imgHtml = '<img class="alert" id="imgAlert" src="' + appRootPath + 'images/alert_no.gif" /><a href="' + appRootPath + 'AlarmSetting/index" id="smsCount">信息(0)</a>';
                        else 
                            imgHtml = '<img class="alert" id="imgAlert" src="' + appRootPath + 'images/alert.gif" /><a href="' + appRootPath + 'AlarmSetting/index" id="smsCount">信息(' + data.SmsNum + ')</a>';

                        if(data.IsAlarmSound)
                            $('#spanAlarmSound').html('<embed src="' + appRootPath + 'Content/Alarm.mp3" autostart="true" loop="1" playcount="1" hidden="true"></embed>');
                        else
                            $('#spanAlarmSound').html('');
                    }

                    spanImg.append(imgHtml);
               }
            });
           setTimeout(reinitSmsCount, 30000);
        }
    </script>
