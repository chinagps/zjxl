    <script type="text/javascript">
        var URL_GetCurrentShortInfoListWithVehicle = '/Position/GetCurrentShortInfoListWithVehicle';
        var URL_EleLimit = '/LocationService/CheckMarkLimit';

        $(document).ready(function () {

            map = new BaiduMap();
            map.init('dvMap', new Point('', 106.00000, 35.00000), 5);

            winResize();
            $(window).resize(winResize);
        });

        function winResize() {//debugger;
            var _winWidth = getWindowWidth();
            _winWidth = _winWidth < 1024 ? 1024 : _winWidth;
            _winWidth = _winWidth - 283;

            if ($.browser.msie) {
                _winWidth -= 4;
            }

            var _MapHeight = $(window).height() - $("#header").height();
            $('#content,.map-area,#side-list,#s_btn').css("height", _MapHeight + "px");
            _MapHeight -= 30;
            $("#dvMap").css({ width: _winWidth + "px", height: _MapHeight + "px", clip: "rect(0px " + _winWidth + "px " + _MapHeight + "px 0px)" });
        }
    </script>
    
