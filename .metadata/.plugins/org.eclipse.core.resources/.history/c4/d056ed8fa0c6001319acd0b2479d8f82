	//添加 车辆类型
	appendVehicleType("vehicleType");
});

function getCurrentUser() {
	var url ="/lmis-portal/login_getCurrentUser.do";
	jQuery.ajax({
	    type: "GET",    
	    url: url,    
	    dataType: "json",
	    success: function(userId){
	    	jQuery("#userId").attr("value",userId);    
	    } 
	});
}

//获得额定载重类型
function getStandardLoad(){
	var url ="/lmis-portal/sysCode_getDataTypeJSON.do";
	jQuery.getJSON(url,{parentId:'290351B127BF4703BAF2023D32E2F5E8'}, function(result) {
		setStandardLoad(result);
	});
}

//设置额定载重
function setStandardLoad(result){
	var standardLoad = document.getElementById("standardLoad");
	jQuery.each(result.data, function(i, code){
		var value = code.code;
		var text = code.name;
		var option = new Option(text,value);
		standardLoad.options.add(option);
	}); 
}

//取所有省份
function getProvince(){
	//&callback=?"注意这个是为了解决跨域访问的问题 
	var url ="/lmis-portal/sysAreaInfo_getAreaJSON.do?areaId=999999";
	jQuery.getJSON(url, function(result) {
		setFromProvince(result);
		setToProvince(result);
	});
}

//send area
function setFromProvince(result){
	var province = document.getElementById("fromProvince");
	jQuery.each(result.data, function(i, area){
		var value = area.areaCode;
		var text = area.areaName;
		var option = new Option(text,value);
		province.options.add(option);
	}); 
}

//按上级ID取子地区
function getFromArea(name){
	if( name=='city' ){
		clearSel(document.getElementById("fromCity")); //清空城市
		var province = document.getElementById("fromProvince");
		if(province.options[province.selectedIndex].value == "") return;
		var areaId = province.options[province.selectedIndex].value;
		var url ="/lmis-portal/sysAreaInfo_getAreaJSON.do?areaId="+areaId;
		jQuery.getJSON(url,null,function call(result){ 
			setFromCity(result); 
		});
	}else if( name=='county'){
		clearSel(document.getElementById("fromCounty")); //清空城区
		var city = document.getElementById("fromCity");
		if(city.options[city.selectedIndex].value == "") return;
		var areaId = city.options[city.selectedIndex].value;
		var url ="/lmis-portal/sysAreaInfo_getAreaJSON.do?areaId="+areaId;
		jQuery.getJSON(url,null,function call(result){ 
			setFromCounty(result); 
		});
	}
}

function setFromCounty(result){
	clearSel(document.getElementById("fromCounty")); //清空城区
	var county = document.getElementById("fromCounty");
	jQuery.each(result.data, function(i, area){
		var value = area.areaCode;
		var text = area.areaName;
		var option = new Option(text,value);
		county.options.add(option);
	}); 
}

//当改变省份时设置城市
function setFromCity(result){
	var city = document.getElementById("fromCity");
	jQuery.each(result.data, function(i, area){
		var value = area.areaCode;
		var text = area.areaName;
		var option = new Option(text,value);
		city.options.add(option);
	}); 
}

//目的地
function setToProvince(result){
	var province = document.getElementById("toProvince");
	jQuery.each(result.data, function(i, area){
		var value = area.areaCode;
		var text = area.areaName;
		var option = new Option(text,value);
		province.options.add(option);
	}); 
}

//按上级ID取子地区
function getToArea(name){
	if( name=='city' ){
		clearSel(document.getElementById("toCity")); //清空城市
		var province = document.getElementById("toProvince");
		if(province.options[province.selectedIndex].value == "") return;
		var areaId = province.options[province.selectedIndex].value;
