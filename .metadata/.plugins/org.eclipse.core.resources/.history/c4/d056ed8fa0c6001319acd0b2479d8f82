//清空下拉列表
function clearSel(oSelect){
	while(oSelect.childNodes.length>0){
		oSelect.removeChild(oSelect.childNodes[0]);
	}
}

//提交表单
function sumberVehicle() {
	var startProvinceId = jQuery("#fromProvince").val(); //发货省份code
	var startProvince = encodeURI(jQuery("#fromProvince :selected").text()); // 发货省份名称
	var startCityId = jQuery("#fromCity").val(); //发货地市code
	var startCity = encodeURI(jQuery("#fromCity :selected").text()); // 发货地市名称
	
	var endProvinceId = jQuery("#toProvince").val(); //收货省份code
	var endProvince = encodeURI(jQuery("#toProvince :selected").text()); // 收货省份名称
	var endCityId = jQuery("#toCity").val(); //收货地市code
	var endCity = encodeURI(jQuery("#toCity :selected").text()); // 收货地市名称
	
	var vehicleType = jQuery("#vehicleType").val(); //车辆类型
	var vehicleNo = encodeURI(jQuery("#vehicleNo").val()); //车牌号
	var standardLoad = jQuery("#standardLoad").val(); //额定载重
	var standardLoadName = encodeURI(jQuery("#standardLoad :selected").text()); 
	var width = jQuery("#width").val(); //宽度
	var length = jQuery("#lengthname").val(); //长度
	var height = jQuery("#height").val(); //高度
	var driverName = encodeURI(jQuery("#driverName").val()); //驾驶员姓名
	var driverPhone = jQuery("#driverPhone").val(); //驾驶员手机
	var companyName = encodeURI(jQuery("#companyName").val()); //真实名称 公司名称
	var idNo = jQuery("#idNo").val(); //证件号码
	var photoUrl = jQuery("#vehicle-photo-url").val(); //图片地址
	var userId = jQuery("#userId").val(); //当前用户Id
	var verifyCode = jQuery("#verifyCode").val();
	var treaty = $("#treaty").attr("checked");
	if(verifyCode=="" || verifyCode.length==0){
		alert("请输入验证码");
		return false;
	}
	if(!treaty){
		alert("未同意发布信息协议");
		return false;
	}
	var check1 = checkVerifyCode(verifyCode);
	if(!check1){
		alert("验证码输入错误");
		$("#verifyCodeImg").trigger("click");
		return false;
	}
	var data = {
		"vehicleSource.publicUserId" : userId,
		"vehicleSource.startProvinceId" : startProvinceId,
		"vehicleSource.startProvince" : startProvince,
		"vehicleSource.startCityId" : startCityId,
		"vehicleSource.startCity" : startCity,
		"vehicleSource.endProvinceId" : endProvinceId,
		"vehicleSource.endProvince" : endProvince,
		"vehicleSource.endCityId" : endCityId,
		"vehicleSource.endCity" : endCity,
		"vehicleSource.vehicleNo" : vehicleNo,
		"vehicleSource.vehicleType" : vehicleType,
		"vehicleSource.standardLoad" : standardLoad,
		"vehicleSource.standardLoadName":standardLoadName,
		"vehicleSource.width" : width,
		"vehicleSource.length" : length,
		"vehicleSource.height" : height,
		"vehicleSource.driverName" : driverName,
		"vehicleSource.driverPhone" : driverPhone,
		"vehicleSource.companyName" : companyName,
		"vehicleSource.idNo" : idNo,
		"vehicleSource.photoUrl" : photoUrl || ""
	}
	$.ajax({
		type:"GET",
		async:false,
		contenttype: "application/json; charset=utf-8",
		url:LMIS_REQUEST_URL+"/noAuth/vehicle/vehicle-source!save.action",
		data : data,
		dataType : "jsonp",
		success : function(json){
			
			
		},
		error:function(){
			alert("提交失败！");
		}
	});
}


function success_jsonpCallback(json){
	alert("提交成功！");
	resetForm("vehicleForm");
	$("#verifyCodeImg").trigger("click");
}

//获得
function getUploadPicURL(text){  
	$("#vehicle-photo-url").val(text);  
}

</script>

