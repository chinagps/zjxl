<script type="text/javascript">


//为了避免jquery中的'$'与其它的'$'冲突,在此将jquery中的'$'重命名为'jQuery'
var jQuery=$;

jQuery.validator.addMethod("floatnum", function(value, element) {
	return this.optional(element) || /^(-?\d+)(\.\d+)?$/.test(value);
}, "只能是浮点数");

jQuery.validator.addMethod("alnum", function(value, element) {
	return this.optional(element) || /^[a-zA-Z0-9]+$/.test(value);
}, "只能包括英文字母和数字");

jQuery.validator.addMethod("mobile", function(value, element) {
  var length = value.length;
  var mobile =  /^[1]\d{10}$/;
  return this.optional(element) || (length == 11 && mobile.test(value));
}, "手机号码格式错误");   


$().ready(function() {

	$("#vehicleForm").validate({
		  rules: {
		   vehicleNo: {required:true,maxlength:10},
		   fromProvince: "required",
		   toProvince: "required",
		   standardLoad: "required",
		   widthname: {required:true,floatnum:true,maxlength:16},  
		   lengthname: {required:true,floatnum:true,maxlength:16},  
		   height: {required:true,floatnum:true,maxlength:16},  
		   driverName: {required:true,maxlength:20},
		   driverPhone: {required:true,mobile:true},
		   companyName: "required",
		   width: "required",
		   companyName:{maxlength:20},
		   idNo: {alnum:true,maxlength:40},
		   gao: {digits:true}
		  },
		  messages: {
		   vehicleNo: {required:"请输车牌号",maxlength:"应小于{0}个字符"}, 
		   fromProvince: "请选择出发省份",
		   toProvince: "请选择到达省份",
		   standardLoad: "请输入额定载重",
		   widthname: {required:"请输入可用宽度",floatnum:"必须为浮点数",maxlength:"应小于{0}位"},
		   lengthname: {required:"请输入可用长度",floatnum:"必须为浮点数",maxlength:"应小于{0}位"},
		   height: {required:"请输入可用高度",floatnum:"必须为浮点数",maxlength:"应小于{0}位"},
		   driverName: {required:"请输入驾驶员姓名",maxlength:"应小于{0}个字符"},
		   driverPhone: {required:"请输入驾驶员手机"}, 
		   companyName: {maxlength:"应小于{0}个字符"},
		   idNo: {maxlength:"应小于{0}个字符"}
		  },
		  invalidHandler : function(){
	         return false;
	    },
	     submitHandler : function(){
	     	   sumberVehicle();
	         return false;
	    }
	});
});




//初始化数据
jQuery(document).ready(function(){
	getProvince();
	getStandardLoad();
	//getCurrentUser();
