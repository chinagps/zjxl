/**
 * 智能物流平台访问地址
 */
 
var contextPath = "/lmis-portal/";

$(document).ready(function() {
	var scrollTopVal = GetQueryString("scrollTopVal");
	if (scrollTopVal != null && scrollTopVal !='') {
		$("html,body").animate({scrollTop:scrollTopVal},0);
	};
	
	//发布信息协议
	$(".treaty").colorbox({iframe:true, width:"90%", height:"90%"});
	//法律声明
	$(".lawexplain").colorbox({iframe:true, width:"90%", height:"90%"});
	
	//Initialize Menu instance(s):
	ddsmoothmenu.init({
		mainmenuid: "menu", //menu DIV id
		customtheme: [], //override default menu CSS background values? Uncomment: ["normal_background", "hover_background"]
		contentsource: "markup" //"markup" or ["container_id", "path_to_menu_file"]
	});
	
	
	initializeMenu("productsMenu", "productsActuator");    
	initializeMenuSub("newPhonesMenu", "newPhonesActuator");
	initializeMenuSub("compareMenu", "compareActuator");
	
});

//跳转到链接页面
function redirectUrl(obj) {
	var y = $(document).scrollTop(); 
	var clickNode = obj.name;
	if (clickNode == null || clickNode == '') {
		obj.href = obj.href + "?scrollTopVal=" + y;
	} else {
		obj.href = obj.href + "?scrollTopVal=" + y + "&clickNode=" + clickNode;
	}
}

String.prototype.trim = function() 
{ 
	return this.replace(/(^\s*)|(\s*$)/g, ""); 
} 

// 获得http请求参数
function GetQueryString(name) 
{ 
	var reg = new RegExp("(^|&)"+ name +"=([^&]*)(&|$)"); 
	var r = window.location.search.substr(1).match(reg); 
	if (r!=null) return unescape(r[2]); return null; 
}

var menus=new Array();
var menusubs=new Array();

if (!document.getElementById)
     document.getElementById = function() { return null; }

//栏目页面 2级菜单点击显示
function initializeMenu(menuId, actuatorId) {
     var menu = document.getElementById(menuId);
     var actuator = document.getElementById(actuatorId);
     if (menu == null || actuator == null) return;
    
     menus.push(menu);

     actuator.parentNode.style.backgroundImage = "";
     actuator.onclick = function() {
         for(var i=0;i<menus.length;i++)if(menus[i]!=menu)menus[i].style.display="none";
         var display = menu.style.display;
         this.parentNode.style.backgroundImage =
             (display == "block") ? "" : "";
         menu.style.display = (display == "block") ? "none" : "block";

         return false;
     }
}

function initializeMenuSub(menuId, actuatorId) {
     var menusub = document.getElementById(menuId);
     var actuatorsub = document.getElementById(actuatorId);
     if (menusub == null || actuatorsub == null) return;
    
     menusubs.push(menusub);

     actuatorsub.parentNode.style.backgroundImage = "";
     actuatorsub.onclick = function() {
         for(var i=0;i<menusubs.length;i++)if(menusubs[i]!=menusub)menusubs[i].style.display="none";
         var display = menusub.style.display;
         this.parentNode.style.backgroundImage =
             (display == "block") ? "" : "";
         menusub.style.display = (display == "block") ? "none" : "block";
         return false;
     }
}

</script>
